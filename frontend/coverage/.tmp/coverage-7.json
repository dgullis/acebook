  <div ref={dropdownRef} className=\"search-results\">\n        \n        {/* if no users found displays message */}\n        {foundUsers.length === 0 ?\n        <div className=\"no-users-found\"> \n            No users found\n        </div>\n            :\n            <>\n            {/* if user(s) found from search displays results showing user profile image and username as clickable link to their profile */}\n            {foundUsers.map((user) => {\n                return ( \n                    <Link to={`/users/${user.username}`} className=\"found-user\" key={user._id}>\n                \n                    <div className=\"user-image-container\">\n                        <img  className=\"user-image\" src={user.image}></img>\n                    </div>\n                    <div className=\"user-name-link\">\n                            {user.username}\n                    </div>\n                    \n                    </Link>\n                )\n            }\n            )}\n            </>\n\n            }\n        </div>\n\n    )\n\n\n}\n"],"names":["Link","useEffect","useRef","SearchResultsDropDown","foundUsers","setShowSearchResults","dropdownRef","handleClickOutside","event","current","contains","target","document","addEventListener","removeEventListener","div","ref","className","length","map","user","to","username","img","src","image","_id"],"mappings":";AAAA,SAASA,IAAI,QAAQ,mBAAkB;AACvC,OAAO,8BAA6B;AACpC,SAASC,SAAS,EAAEC,MAAM,QAAQ,QAAO;AAEzC,YAAY;AACZ,eAAe,SAASC,sBAAuB,EAAEC,UAAU,EAAEC,oBAAoB,EAAE;IAC/E,MAAMC,cAAcJ,OAAO;IAE3BD,UAAU;QACN,MAAMM,qBAAqB,CAACC;YACxB,kDAAkD;YAClD,IAAIF,YAAYG,OAAO,IAAI,CAACH,YAAYG,OAAO,CAACC,QAAQ,CAACF,MAAMG,MAAM,GAAG;gBACpEN,qBAAqB,OAAO,oCAAoC;;YACpE;QACJ;QACA,wEAAwE;QACxEO,SAASC,gBAAgB,CAAC,aAAaN;QAEvC,oDAAoD;QACpD,OAAO;YACHK,SAASE,mBAAmB,CAAC,aAAaP;QAC9C;IACI,GAAG;QAACD;KAAY;IAGxB,qBACI,QAACS;QAAIC,KAAKV;QAAaW,WAAU;kBAGhCb,WAAWc,MAAM,KAAK,kBACvB,QAACH;YAAIE,WAAU;sBAAiB;;;;;iCAI5B;sBAECb,WAAWe,GAAG,CAAC,CAACC;gBACb,qBAC